  },
  "devDependencies": {
    "argg": "0.0.1",
    "codeclimate-test-reporter": "0.0.3",
    "istanbul": ">=0.3.0 <0.4.0-0",
    "rimraf": ">=2.2.8 <3.0.0-0",
    "tape": ">=2.14.0 <3.0.0-0"
